f = open("p9.txt", "r")
count = 0
sr_1 = 0
sr_2 = 0
for s in f.readlines():
    nums = list(map(int,s.split()))
    dct = {num if nums.count(num)>1 else 0 : nums.count(num) for num in nums}
    keys = list(dct.keys())
    del(keys[0])
    if len(keys)>0:
        sr_1 = (max(nums)+min(nums))/2
        sr_2 = sum(key*dct[key] for key in keys)/sum(dct[key] for key in keys)
        if sr_1>sr_2:
            count += 1
    
print(count)
f.close()
